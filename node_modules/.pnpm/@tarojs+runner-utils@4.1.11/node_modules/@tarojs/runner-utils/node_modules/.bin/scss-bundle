#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/joyy/Code/XC_project/XC_project/node_modules/.pnpm/scss-bundle@3.1.2/node_modules/scss-bundle/dist/cli/node_modules:/Users/joyy/Code/XC_project/XC_project/node_modules/.pnpm/scss-bundle@3.1.2/node_modules/scss-bundle/dist/node_modules:/Users/joyy/Code/XC_project/XC_project/node_modules/.pnpm/scss-bundle@3.1.2/node_modules/scss-bundle/node_modules:/Users/joyy/Code/XC_project/XC_project/node_modules/.pnpm/scss-bundle@3.1.2/node_modules:/Users/joyy/Code/XC_project/XC_project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/joyy/Code/XC_project/XC_project/node_modules/.pnpm/scss-bundle@3.1.2/node_modules/scss-bundle/dist/cli/node_modules:/Users/joyy/Code/XC_project/XC_project/node_modules/.pnpm/scss-bundle@3.1.2/node_modules/scss-bundle/dist/node_modules:/Users/joyy/Code/XC_project/XC_project/node_modules/.pnpm/scss-bundle@3.1.2/node_modules/scss-bundle/node_modules:/Users/joyy/Code/XC_project/XC_project/node_modules/.pnpm/scss-bundle@3.1.2/node_modules:/Users/joyy/Code/XC_project/XC_project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../scss-bundle@3.1.2/node_modules/scss-bundle/dist/cli/main.js" "$@"
else
  exec node  "$basedir/../../../../../../scss-bundle@3.1.2/node_modules/scss-bundle/dist/cli/main.js" "$@"
fi
